package patterns;

public class Pattern6 {
    public static void main(String[] args) {
        for(int i=1;i<=5+1;i++)
        {
            for(int j=1;j<=5+1-i;j++)
            {
                System.out.print(j);
            }
            System.out.println();
        }
    }
}

